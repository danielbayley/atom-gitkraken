{
  "name": "gitkraken",
  "version": "0.4.2",
  "description": "Open current project in GitKraken.",
  "keywords": [
    "project",
    "git",
    "gitkraken",
    "workflow"
  ],
  "homepage": "https://atom.io/packages/gitkraken",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbayley/atom-gitkraken.git"
  },
  "bugs": {
    "url": "https://github.com/danielbayley/atom-gitkraken/issues"
  },
  "author": "Daniel Bayley <daniel.bayley@me.com> (https://github.com/danielbayley)",
  "private": true,
  "license": "MIT",
  "os": [
    "darwin",
    "linux"
  ],
  "dependencies": {
    "sub-atom": "^1.1.0"
  },
  "engines": {
    "atom": "*"
  },
  "configSchema": {
    "singleInstance": {
      "description": "Limit to a single instance of the application, else spawn one per project.",
      "type": "boolean",
      "default": false
    }
  }
}
